{
  "name": "arx/project",
  "description": "Arx Starter Project Template",
  "keywords": ["framework", "laravel", "Saas", "Starter Kit"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "arx/core": "5.2.*",
    "arx/arxmin": "5.2.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "autoload": {
    "classmap": [
      "database",
      "modules"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [

    ],
    "post-install-cmd": [

    ],
    "pre-update-cmd": [

    ],
    "post-update-cmd": [

    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability" : "dev"
}
