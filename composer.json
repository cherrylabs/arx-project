{
    "name": "arx/projectcontrib",
    "description": "Default starter project for new contributor",
    "keywords": ["arx", "framework", "starter kit"],
    "require": {
        "arx/core" : "*"
    },
    "license": "MIT",
    "require-dev": {

    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [

        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
