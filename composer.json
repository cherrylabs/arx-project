{
  "name": "arx/project",
  "description": "Starter project for Laravel 5",
  "keywords": [
    "framework",
    "laravel",
    "arx"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "arx/core": "dev-master",
    "illuminate/html": "5.x",
    "illuminate/workbench": "dev-master",
    "doctrine/dbal": "~2.3",
    "kriswallsmith/assetic" : "*",
    "fzaninotto/faker" : "*",
    "barryvdh/laravel-ide-helper": "~2.0",
    "filp/whoops": "~1.0",
    "barryvdh/laravel-debugbar": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database",
      "workbench"
    ],
    "psr-4": {
      "App\\": "app/controllers/",
      "App\\": "app/models/",
      "App\\": "app/interfaces/",
      "App\\": "app/traits/",
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [

    ],
    "post-update-cmd": [

    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
